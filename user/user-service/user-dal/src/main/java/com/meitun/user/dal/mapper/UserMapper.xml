<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.meitun.user.dal.mapper.UserMapper">
  <resultMap id="users" type="com.meitun.user.dal.domain.UserDO">
      <result column="id" property="id"></result>
      <result column="name" property="name"></result>
  </resultMap>

    <select  id="getUsers" resultMap="users">
        select id,name from user

    </select>

    <select  id="count" resultType="int">
        select count(*) from user

    </select>

    <select id="getUserById" parameterType="Long" resultMap="users">
        select  id,name from user where id = #{id}

    </select>

</mapper>